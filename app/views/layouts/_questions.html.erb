<% @answers.each do |a|%>
    <%= label_tag :answer_name , a.answer_name %>
    <%= fields_for "data_from_test[]", @user_answer do |u| %>
        <%= u.hidden_field :test_id, value: @test.id %>
        <%= u.hidden_field :answer_id, value: a.id %>

        <%= u.check_box :answer_value, include_hidden: false , class:"checkbox" %>
    <% end %>
<% end %>

