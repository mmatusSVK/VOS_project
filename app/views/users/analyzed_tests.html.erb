<% provide(:title, 'AnalyzovanÃ© testy') %>
<% make_all_current_null %>

<div class="row">

  <div class="col-md-9 my_content well">

    <% @one_test_data.each do |key, item| %>
        <% test = item.test %>
        <h3><%= link_to test.test_name, user_test_concrete_test_path(@login_user, test, :starting_date => item.starting_date) %></h3>

        <% time = DateTime.parse(key) %>
        <% hours = time.strftime("%k").to_i + 2 %>
        <p><%= time.strftime("%Y %B %d, #{hours}:%M:%S") %></p>
    <% end %>

  </div>

</div>

